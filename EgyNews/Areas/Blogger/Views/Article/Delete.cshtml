@model Article

<h1> Delete Article </h1>

<form asp-action="Delete">
	<div class="form-group">
		<label asp-for="Title"></label>
		<input asp-for="Title" class="form-control" disabled />
	</div>
	<div class="form-group">
		<label asp-for="Content"></label>
		<textarea asp-for="Content" class="form-control" disabled > </textarea>
	</div>
	<div class="form-group">
		<label asp-for="PublishDate"></label>
		<input asp-for="PublishDate" type="date" class="form-control" disabled />
	</div>
	<div class="form-group">
		<label asp-for="Category"></label>
		<input asp-for="Category.Name" class="form-control" disabled />
	</div>
	<div class="form-group">
		<label asp-for="ImageUrl"></label>
		<input asp-for="ImageUrl" class="form-control" disabled />
	</div>
	<div class="form-group">
		<input type="submit" value="Delete" class="btn btn-danger" />
		<a asp-action="Index" class="btn btn-secondary">Back to List</a>
	</div>
</form>