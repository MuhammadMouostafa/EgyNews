@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>


<h2>ViewData Numbers:</h2>
<ul>
    @if (ViewData["NumbersList"] != null)
    {
        foreach (var num in (List<int>)ViewData["NumbersList"])
        {
            <li>@num</li>
        }
    }
    else
    {
        <li>ViewData is lost after redirection!</li>
    }
</ul>

<h2>ViewBag Numbers:</h2>
<ul>
    @if (ViewBag.NumbersList != null)
    {
        foreach (var num in ViewBag.NumbersList)
        {
            <li>@num</li>
        }
    }
    else
    {
        <li>ViewBag is lost after redirection!</li>
    }
</ul>

<h2>TempData Numbers:</h2>
<ul>
    @if (TempData["NumbersList"] != null)
    {
        foreach (var num in (List<int>)TempData["NumbersList"])
        {
            <li>@num</li>
        }
    }
    else
    {
        <li>TempData is lost if read once!</li>
    }
</ul>
